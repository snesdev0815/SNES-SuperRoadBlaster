	.db :excA,
	.db 2," x:   ",TC_hToS
	.dw excX
	.db :excX,
	.db 2," y: ",TC_hToS
	.dw excY
	.db :excY,
	.db 2,TC_brk,"flag:  ",TC_hToS
	.dw excFlags
	.db :excFlags,
	.db 1," stck:",TC_hToS
	.dw excStack
	.db :excStack,
	.db 2," pc:",TC_hToS
	.dw excPc
	.db :excPc,
	.db 2,TC_brk,"dp:  ",TC_hToS
	.dw excDp
	.db :excDp,
	.db 2," pb:    ",TC_hToS
	.dw excPb
	.db :excPb,
	.db 1," db:  ",TC_hToS
	.dw excDb
	.db :excDb,
	.db 1,TC_brk,TC_end

