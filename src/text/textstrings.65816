T_EXCP_exception:
    .db TC_pos
    .dw $42
    .db TC_sub
    .dw T_EXCP_starLine.PTR
    .db "a fatal error has occured!",TC_brk
    .db TC_sub
    .dw T_EXCP_starLine.PTR
    .db "error-message:"
    .db TC_pos
    .dw $1c2
    .db TC_sub
    .dw T_EXCP_starLine.PTR
    .db "Data access errors may be",TC_brk
    .db "caused by slow storage media",TC_brk
    .db "or bad and/or missing files.",TC_brk,
    .db TC_sub
    .dw T_EXCP_starLine.PTR
    .db TC_end

T_EXCP_starLine:
	.db "****************************",TC_brk,TC_end
	
T_EXCP_cpuInfo:
	.db "cpu status:",TC_brk
	.db "a:   ",TC_hToS
